<script>
    $(document).ready(function(e){
        init(e);
        function renderAll(){
            loadSocialFeeds(render_social);
            var banners = getBanners();
            var mobile_banners = getRepoDetailsByName("Mobile Banners");
            var feature_items = getFeatureList().splice(0, 3);
            var promotions = getPromotionsList().sortBy(function(o){ return o.start_date });
            renderBanner('#banner_template','#home_banner',banners);
            if(mobile_banners != undefined)
                renderMobileBanner('#mobile_banner_template', '#mobile_banner', mobile_banners.images);
            renderFeatureItems('#feature_container','#feature_template', feature_items);
            var published_promos = [];
            $.each( promotions , function( key, val ){
                today = moment();
                webDate = moment(val.show_on_web_date);
                if (today.tz(site_json.time_zone) >= webDate.tz(site_json.time_zone)) {
                    published_promos.push(val)
                }
            });
            if(published_promos.length > 0){
                // renderPromotions("#promos_container","#promos_template", published_promos.reverse().slice(0,3));
            }
            // show_content();
            
            $('.flexslider').flexslider({
                animation: "slide",
                allowOneSlide: false,
            });
            $(".flex-direction-nav").hide();
            renderSitePopup();
        }
         function render_social(){
             console.log("getSocialFeed",getSocialFeed());
            var social_feed = getSocialFeed()
            var latest_tweet = social_feed.twitter[0]
            var latest_fb_post = social_feed.facebook[0]
            console.log("latestfb" ,latest_fb_post);
            if(latest_tweet.text.length > 100){
                $('#latest_tweet').text(latest_tweet.text.substr(0,100) + "...")
            }
            else{
                $('#latest_tweet').text(latest_tweet.text)
            }
            $('#latest_tweet_link').attr('href', latest_tweet.link)
            var tweet_date = new Date(latest_tweet.created_at.split(',')[0])
            $('#latest_tweet_date').text(get_month(tweet_date.getMonth()) + " " + tweet_date.getDate())
            
            // console.log(latest_fb_post);
            if(latest_fb_post.text.length > 150){
                $('#latest_fb').text(latest_fb_post.text.substr(0,150) + "...")
            }
            else{
                $('#latest_fb').text(latest_fb_post.text)
            }
            $('#latest_fb_link').attr('href', latest_fb_post.link)
            var fb_date = new Date(latest_fb_post.created_at.split(',')[0])
            $('#latest_fb_date').text(get_month(fb_date.getMonth()) + " " + fb_date.getDate())
        }
        loadMallData(renderAll); 
    });
    $(window).load(function(e){
        show_content();
    });
</script>