<script>
    $(document).ready(function(e){
        init(e);
        loadMallDataCached(renderPage);
    })
    
   function renderPage(){
        var pathArray = window.location.pathname.split( '/' );
        var slug = pathArray[pathArray.length-1];
        
        prefix = get_prefix();
        $.validate();
         var contest_details = getContestBySlug('yubasutter-kids-club'); 
         var property = getPropertyDetails ();
         console.log(contest_details, property);
         $('#contest_id').val(contest_details.id);
         $('#property_id').val(property.id);
        $('#contest_form').submit(function(e){
            
            e.preventDefault();
            $('.contest_btn').prop('disabled', true);
            // submit_contest(slug);
            submit_contest($(this).serialize(), contest_details.slug);
            // $('html, body').animate({scrollBottom : 0},800);
        });
    };
    $(window).load(function(e){
        show_content();
    });
</script>